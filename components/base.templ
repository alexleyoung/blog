package components

templ Base(title string) {
<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{title}</title>
</head>

<body>
	<nav>
		<a href={templ.SafeURL("/")}>Home</a>
		<a href={templ.SafeURL("/yap")}>Yap</a>
	</nav>
	{ children... }
</body>

</html>
}
